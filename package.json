{
  "name": "@vippsno/vipps-sdk",
  "version": "0.9.4",
  "homepage": "https://www.npmjs.com/package/@vippsno/vipps-sdk",
  "repository": {
    "type": "git",
    "url": "https://github.com/vippsas/vipps-node"
  },
  "bugs": {
    "url": "https://github.com/vippsas/vipps-node/issues"
  },
  "engines": {
    "node": ">=14.x.x",
    "npm": ">=7.x.x"
  },
  "description": "Vipps SDK",
  "main": "./lib/index.cjs",
  "type": "module",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs"
    }
  },
  "files": [
    "lib",
    "LICENSE"
  ],
  "scripts": {
    "test": "jest",
    "build": "node esbuild.config.js && tsc && api-extractor run --local --verbose",
    "autogen:checkout": "swagger-typescript-api -p \"https://vippsas.github.io/vipps-developer-docs/redocusaurus/checkout-swagger-id.yaml\"  -o ./src/@types -n checkout.types.ts --no-client --union-enums --type-prefix Checkout",
    "autogen:epayment": "swagger-typescript-api -p \"https://vippsas.github.io/vipps-developer-docs/redocusaurus/epayment-swagger-id.yaml\"  -o ./src/@types -n epayment.types.ts --no-client --union-enums --type-prefix EPayment",
    "autogen:access-token": "swagger-typescript-api -p \"https://vippsas.github.io/vipps-developer-docs/redocusaurus/access-token-swagger-id.yaml\"  -o ./src/@types -n access-token.types.ts --union-enums --no-client",
    "autogen:webhooks": "swagger-typescript-api -p \"https://vippsas.github.io/vipps-developer-docs/redocusaurus/webhooks-swagger-id.yaml\"  -o ./src/@types -n webhooks.types.ts --no-client --union-enums --type-prefix Webhooks",
    "audit": "npm audit --registry=https://registry.npmjs.org/",
    "lint": "eslint src --color",
    "prettier": "prettier -c src",
    "prettier:fix": "prettier -w src",
    "sdk-version": "echo $npm_package_version"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@microsoft/api-extractor": "^7.34.4",
    "@types/async-retry": "^1.4.5",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.2",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "esbuild": "^0.17.14",
    "eslint": "^8.35.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.4",
    "swagger-typescript-api": "^12.0.3",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "async-retry": "^1.3.3",
    "uuid": "^9.0.0"
  }
}
